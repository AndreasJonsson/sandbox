= Macro Directory =

[[Import Google Gadget&gt;&gt;Macros.GoogleGadgetsImport]]

#set($macroDescrs = $macros.macroDescriptors)
#foreach($m in $macroDescrs)
  #set($macroId = $m.getId().id)
