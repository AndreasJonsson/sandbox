<property>
<category>Information</category>
</property>
<property>
<content>#panelheader($msg.get("xe.statistics.pages.mostedited"))
#if($xwiki.statsService.enabled)
#set($scope=$xwiki.criteriaService.scopeFactory.ALL_PAGES)
#if($request.period=="all")
#set($period=$xwiki.criteriaService.periodFactory.ALL_TIME)
#elseif($request.period)
#set($period=$statsgroovy.getPeriodCriteria())
#else
#set($period=$xwiki.criteriaService.periodFactory.currentMonth)
#end
#if($request.nb)
#set($nb = $xwiki.parseInt($request.nb))
#else
#set($nb = 10)
#end
#set($interval=$xwiki.criteriaService.rangeFactory.createHeadRange($nb))
#set($stats=$xwiki.statsService.getDocumentStatistics("save",$scope,$period,$interval))
#if($stats.size() &gt; 0)
#printPageStats($stats)
#else
#printNoStatisticsData()
#end
#else
#printStatisticsDisabled()
#end
#panelfooter()
#includeMacros("Stats.Macros")</content>
</property>
<property>
<description>Displays the most edited pages</description>
